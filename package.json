    "@typescript-eslint/parser": "^4.26.1",
    "esbuild": "^0.12.8",
    "eslint": "^7.28.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.23.4",
    "nodemon": "^2.0.7",
